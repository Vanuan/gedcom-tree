{
  "name": "gedcom-tree",
  "version": "0.0.1",
  "devDependencies": {
    "tape": "~1.1.0",
    "brfs": "~0.0.9",
    "browserify": "~2.34.0"
  },
  "browserify": { "transform": ["brfs"] },
  "testling": {
    "files": "test/*.js",
    "browsers": [ "ie9", "ff/26", "chrome/20","chrome/30", "chrome/33" ]
  },
  "scripts": {
    "test": "node test/*.js",
    "build-test": "mkdir -p build; browserify -t brfs test/*.js > build/tests.js",
    "build": "browserify --standalone GedcomTree gedcom-tree.js > build/gedcom-tree.js"
  }
}
